#include <iostream>
using namespace std;

int main() {
    int A, B, C, D, DIFERENCA;

    cin >> A >> B >> C >> D; // read four integers

    DIFERENCA = (A * B) - (C * D); // calculate difference

    cout << "DIFERENCA = " << DIFERENCA << endl; // print result in required format

    return 0;
}
