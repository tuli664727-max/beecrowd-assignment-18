#include <iostream>
using namespace std;

int main() {
    int A, B, SOMA;

    cin >> A >> B; // read two integers

    SOMA = A + B; // calculate sum

    cout << "SOMA = " << SOMA << endl; // print result in required format

    return 0;
}
