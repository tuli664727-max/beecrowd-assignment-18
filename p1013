#include<iostream>
using namespace std; 
int main() {   
int a, b, c;  

cin >> a >> b >> c;     
int maior = (a > b ? a : b);  
maior = (maior > c ? maior : c);  
cout << maior << " eh o maior" << endl;     

return 0; 
    
}
