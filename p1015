#include <iostream> 
#include <iomanip>
#include <cmath> 
// For sqrt and pow using namespace std; 
int main() {    
double x1, y1, x2, y2;   
// Read input     cin >> x1 >> y1 >> x2 >>y2;     
// Calculate distance    
double distance = sqrt(pow(x2 - x1, 2) +pow(y2 - y1, 2));     
// Print output with 4 decimal places  
cout << fixed << setprecision(4) << dist << '\n'; 
return 0; 

}
